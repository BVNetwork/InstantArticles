@using BVNetwork.InstantArticles.Business.Interfaces
@using EPiServer.Core
@using EPiServer.Shell
@model IList<ContentReference>

@if (Model.Any())
{
    foreach (var author in Model)
    {
        Html.RenderPartial(@Paths.PublicRootPath + "BVNetwork.InstantArticles/Views/Shared/InstantArticleAuthor.cshtml", author.Get<IContent>() as IInstantArticleAuthor);
    }
}