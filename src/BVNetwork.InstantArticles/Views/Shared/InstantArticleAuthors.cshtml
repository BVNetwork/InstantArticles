@using AlloyInstantArticles.Business
@using BVNetwork.InstantArticles.Models.Interfaces
@using EPiServer.Core
@model IList<ContentReference>

@if (Model.Any())
{
    foreach (var author in Model)
    {
        Html.RenderPartial("InstantArticleAuthor", author.Get<IContent>() as IInstantArticleAuthor);
    }
}